<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="32px"
        android:id="@+id/root_layout"
        >
        <View
            android:layout_width="match_parent"
            android:layout_height="4px"
            android:background="@drawable/line_dash_color_dddddd"
            android:layerType="software"
            />
        <com.yxld.xzs.view.AlignLeftRightTextView
            android:id="@+id/tv_xunjianshijian"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:left_text="巡检时间"/>

        <com.yxld.xzs.view.AlignLeftRightTextView
            android:id="@+id/tv_xunjianluxian"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:left_text="巡检路线" />

        <com.yxld.xzs.view.AlignLeftRightTextView
            android:id="@+id/tv_kaishiquyu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:left_text="开始区域"/>

        <com.yxld.xzs.view.AlignLeftRightTextView
            android:id="@+id/tv_jieshuquyu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:left_text="结束区域"/>

        <com.yxld.xzs.view.AlignLeftRightTextView
            android:id="@+id/tv_gongjixunjiandian"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:left_text="共计巡检点" />
        <com.yxld.xzs.view.AlignLeftRightTextView
            android:id="@+id/tv_xunjianrenyuan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:left_text="巡检人员" />
        <com.yxld.xzs.view.AlignLeftRightTextView
            android:id="@+id/tv_xunjianstatus"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:left_text="巡检状态" />
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoFrameLayout>
